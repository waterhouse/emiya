              (tuples 2 args))))
(mac destructuring-assign (x y) ;most inefficient code ever
  (if no.x
      nil
      (isa x 'sym)
      `(= ,x ,y)
      acons.x
      (w/uniq (gy ga gb)
        `(let ,gy ,y
           (with (,ga (car ,gy) ,gb (cdr ,gy))
             (destructuring-assign ,(car x) ,ga)
             (destructuring-assign ,(cdr x) ,gb))))))
