(def is-square-mod (a p) ;Euler's criterion (p is prime)
  (if prime.p
      (or (is p 2) (is 1 (mod-expt a (/ dec.p 2) p)))
      (find-int [is 0 (mod (- square._ a) p)] 0 dec.p)))

