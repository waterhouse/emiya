   (obj uint64 $.c-_uint64
        u64 $.c-_uint64
        uint $.c-_uint64
        int64 $.c-_int64
        i64 $.c-_int64
        int $.c-_int64
        int32 $.c-_int32
        uint32 $.c-_uint32
