(def str-colored (str (o color 'red) (o bold t))
  (string "\033["
          (if bold 1 0)
          ";"
          (case color
            black 30
            red 31
            green 32
            yellow 33
            blue 34
            magenta 35
            cyan 36
            white 37)
          "m"
          str
          "\033[m"))

(def pr-colored (str (o color 'red) (o bold t))
  (pr:str-colored str color bold))

